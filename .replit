modules = ["nodejs-20", "web"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5173
externalPort = 5173

[[ports]]
localPort = 37391
externalPort = 3002

[[ports]]
localPort = 40649
externalPort = 3001

[[ports]]
localPort = 43639
externalPort = 3000

[[ports]]
localPort = 45355
externalPort = 3003

[env]
PORT = "5000"

[agent]
integrations = ["javascript_log_in_with_replit:1.0.0", "spotify:1.0.0", "javascript_websocket:1.0.0", "javascript_database:1.0.0", "javascript_supabase:1.0.0"]
expertMode = true

[workflows]
runButton = "Start App"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

